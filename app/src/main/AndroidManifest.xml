        <activity
            android:name="com.christian.minhacolecao2018.MoedasNacionais"
            android:label="Moedas Brasileiras"></activity>
        <activity
            android:name="com.christian.minhacolecao2018.Notas"
            android:label="Notas"></activity>
        <activity
            android:name="com.christian.minhacolecao2018.NotasBr"
            android:label="Notas Brasileiras"></activity>
        <activity
            android:name="com.christian.minhacolecao2018.TPesquisa"
            android:label="Pesquisar"></activity>
        <activity
